      "module": "commonjs",
      "moduleResolution": "node",
      "pretty": true,
      "sourceMap": true,
      "target": "es6",
      "outDir": "./dist",
      "baseUrl": "./lib",
      "esModuleInterop": false,
      "allowJs": true,
      "declaration": false
  
    },

    "include": [
      "lib/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }